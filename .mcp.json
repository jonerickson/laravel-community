{
    "mcpServers": {
        "laravel-boost": {
            "command": "php",
            "args": [
                "artisan",
                "boost:mcp"
            ]
        },
        "playwright": {
            "type": "stdio",
            "command": "npx",
            "args": ["@playwright/mcp@latest"]
        },
        "herd": {
            "command": "php",
            "args": [
                "/Applications/Herd.app/Contents/Resources/herd-mcp.phar"
            ],
            "env": {
                "SITE_PATH": "/Users/jonerickson/Code/mi"
            }
        },
        "sentry": {
            "command": "npx",
            "args": ["-y", "mcp-remote@latest", "https://mcp.sentry.dev/mcp"]
        }
    }
}
