{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "jonerickson/laravel-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-imap": "*",
        "ext-intl": "*",
        "ext-libxml": "*",
        "ext-pcntl": "*",
        "api-platform/laravel": "^4.2.10",
        "beyondcode/laravel-mailbox": "^5.0.2",
        "fakerphp/faker": "^1.24.1",
        "filament/filament": "^4.3.1",
        "filament/spatie-laravel-settings-plugin": "^4.3.1",
        "inertiajs/inertia-laravel": "^2.0.15",
        "lab404/laravel-impersonate": "^1.7.7",
        "laravel/cashier": "^16.2.0",
        "laravel/framework": "^12.51.0",
        "laravel/horizon": "^5.40.2",
        "laravel/nightwatch": "^1.22.1",
        "laravel/passport": "^13.4.1",
        "laravel/scout": "^10.24.0",
        "laravel/serializable-closure": "^2.0.9",
        "laravel/socialite": "^5.24",
        "laravel/telescope": "^5.16",
        "league/flysystem-aws-s3-v3": "^3.30.1",
        "meilisearch/meilisearch-php": "^1.16.1",
        "nexxai/laravel-cfcache": "^0.3.0",
        "phiki/phiki": "^1.1.6",
        "sendgrid/sendgrid": "^8.1.11",
        "sentry/sentry-laravel": "^4.20",
        "spatie/enum": "^3.13",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/laravel-csp": "^3.21",
        "spatie/laravel-data": "^4.19.1",
        "spatie/laravel-google-fonts": "^1.4.4",
        "spatie/laravel-pdf": "^1.9",
        "spatie/laravel-permission": "^6.24",
        "spatie/laravel-settings": "^3.6.0",
        "spatie/laravel-typescript-transformer": "^2.5.2",
        "spatie/laravel-webhook-server": "^3.8.3",
        "staudenmeir/eloquent-has-many-deep": "^1.21.2",
        "symfony/expression-language": "^7.4",
        "tightenco/ziggy": "^2.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^4.0.7",
        "barryvdh/laravel-ide-helper": "^3.6.1",
        "beyondcode/laravel-query-detector": "^2.1",
        "driftingly/rector-laravel": "^2.1.7",
        "larastan/larastan": "^3.8.1",
        "laravel/boost": "^2.0.4",
        "laravel/facade-documenter": "dev-main",
        "laravel/pail": "^1.2.4",
        "laravel/pint": "^1.26.0",
        "laravel/sail": "^1.51",
        "laravel/tinker": "^2.11.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.3",
        "pestphp/pest": "^4.3.1",
        "pestphp/pest-plugin-arch": "^4.0.0",
        "pestphp/pest-plugin-laravel": "^4.0.0",
        "rector/rector": "^2.2.14"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laravel/facade-documenter"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade --ansi",
            "@php artisan api-platform:install --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "install-hooks": [
            ".githooks/install.sh"
        ],
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "@php artisan passport:keys --force",
            "@php artisan boost:update -n --ansi",
            "@php artisan google-fonts:fetch",
            "@php artisan storage:link",
            "npm install",
            "npm run build",
            "@php artisan app:install --name=\"Test User\" --email=\"test@test.com\" --password=\"password\" -n"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -k -c \"#2563eb,#d4d4d8,#4f46e5\" \"php artisan horizon\" \"php artisan pail\" \"npm run dev\" --names=queue,logs,vite"
        ],
        "dev:ssr": [
            "npm run build:ssr",
            "@dev"
        ],
        "analyze": "vendor/bin/phpstan analyse --memory-limit=2G -v",
        "cs-fix": "vendor/bin/pint --parallel",
        "docs:models": [
            "@php artisan ide-helper:models -nRW",
            "@cs-fix app/Models"
        ],
        "facades": [
            "@php -f vendor/bin/facade.php --  \\\\App\\\\Facades\\\\ExpressionLanguage \\\\App\\\\Facades\\\\PaymentProcessor \\\\App\\\\Facades\\\\PayoutProcessor \\\\App\\\\Facades\\\\SupportTicket"
        ],
        "ide": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -nRW",
            "@cs-fix app/Models"
        ],
        "lint": "@cs-fix",
        "rector": "./vendor/bin/rector process --ansi",
        "reset": [
            "@composer install --no-interaction --no-progress",
            "npm ci --no-audit --no-fund",
            "npm run build",
            "@php artisan migrate:fresh --seed",
            "@php artisan horizon:purge",
            "@php artisan optimize:clear",
            "@ide",
            "@cs-fix"
        ],
        "tc": "@test-coverage",
        "test": "@php artisan test --no-coverage --parallel",
        "test-coverage": "@php artisan test --coverage --parallel --min=60",
        "test-filter": "@php artisan test --no-coverage --filter",
        "test-lint": "@cs-fix --test",
        "test-suite": [
            "@analyze",
            "@test"
        ],
        "test-types": "@analyze",
        "test:filter": "@test-filter",
        "test:lint": "@test-lint",
        "test:suite": "@test-suite",
        "test:types": "@analyze",
        "tf": "@test-filter",
        "tl": "@test-lint",
        "ts": "@test-suite",
        "tt": "@test-types",
        "types": "@php artisan  typescript:transform --format"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
